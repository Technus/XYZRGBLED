<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<VBox spacing="4.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.technus.xyzrgbled.javafx.pwmSetting.PwmSettingController">
   <children>
      <HBox spacing="4.0">
          <BorderPane>
              <center>
                  <StackPane BorderPane.alignment="CENTER">
                      <children>
                          <Circle fx:id="settingCircle" fill="white" radius="12.0" stroke="BLACK" strokeType="INSIDE" />
                      </children>
                  </StackPane>
              </center>
          </BorderPane>
          <Spinner fx:id="settingSpinner" editable="true" maxWidth="1.7976931348623157E308" prefWidth="176.0" HBox.hgrow="ALWAYS" />
      </HBox>
      <Slider fx:id="settingSlider" showTickMarks="true">
         <padding>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </padding>
      </Slider>
   </children>
</VBox>
